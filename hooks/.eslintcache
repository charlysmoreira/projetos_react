 = useState(1)\n    const [fatorial, setFatorial] = useState(1)\n\n    /* Pode ser assim, funcao anonima\n    useEffect(function(){\n        setFatorial(calcFatorial(number))\n    }, [number]) */\n\n    useEffect(() => setFatorial(calcFatorial(number)), [number])\n\n    useEffect(function(){\n        if(fatorial > 1000000){\n            document.title = \"Eita!!\"\n        }\n    }, [fatorial])\n\n    //Ex: 01\n    // essas funcao hook useEffect tem como parametro uma funcao e um array com os atributos que sofrem alteracao\n    const [status, setStatus] = useState('Ímpar')\n    useEffect(() => {setStatus(calNumerPar(number))})\n    \n    return (\n        <div className=\"UseEffect\">\n            <PageTitle\n                title=\"Hook UseEffect\"\n                subtitle=\"Permite executar efeitos colaterais em componentes funcionais!\"\n            />\n            <SectionTitle title= \"Exercicio - #1\"></SectionTitle>\n            <div className=\"center\">\n                <div>\n                    <span className=\"text\">Fatorial:</span>\n                    <span className=\"text red\">{fatorial}</span>\n                </div>\n\n                <input type=\"number\" className=\"input\" \n                    value={number} onChange={e => {setNumber(e.target.value)}}/>\n            </div>\n\n            <SectionTitle title= \"Exercicio - #2\"></SectionTitle>\n            <div className=\"center\">\n                <div>\n                    <span className=\"text\">Status:</span>\n                    <span className=\"text red\">{status}</span>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default UseEffect\n","C:\\Users\\PinheirC\\hp_compliance\\projeto_react\\projetos_react\\hooks\\src\\views\\examples\\UseState.jsx",[],"C:\\Users\\PinheirC\\hp_compliance\\projeto_react\\projetos_react\\hooks\\src\\views\\examples\\UseCallback.jsx",[],"C:\\Users\\PinheirC\\hp_compliance\\projeto_react\\projetos_react\\hooks\\src\\views\