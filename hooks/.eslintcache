      <button className=\"btn\" onClick={() => addNumber(-1)}>-1</button>\n                    <button className=\"btn\" onClick={() => addNumber(1)}>+1</button>\n                </div>\n            </div>\n            <SelectionTitle title= \"Exercicio #2\"/>\n            <div className=\"center\">\n                <span className=\"text\">{text}</span>\n                <span className=\"text\">{number}</span>\n                <div>\n                    <buttun className=\"btn\" onClick={() => setNumber(number - 1)}>-1</buttun>\n                    <buttun className=\"btn\" onClick={() => setNumber(number + 1)}>+1</buttun>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default UseContext\n","C:\\Users\\PinheirC\\hp_compliance\\projeto_react\\projetos_react\\hooks\\src\\components\\layout\\PageTitle.jsx",[],"C:\\Users\\PinheirC\\hp_compliance\\projeto_react\\projetos_react\\hooks\\src\\components\\layout\\SectionTitle.jsx",[],"C:\\Users\\PinheirC\\hp_compliance\\projeto_react\\projetos_react\\hooks\\src\\views\\examples\\UseCallbackButtons.jsx",[],"C:\\Users\\PinheirC\\hp_compliance\\projeto_react\\projetos_react\\hooks\\src\\data\\DataContext.js",[],"C:\\Users\\PinheirC\\hp_compliance\\projeto_react\\projetos_react\\hooks\\src\\data\\Store.jsx",[],"C:\\Users\\PinheirC\\hp_compliance\\projeto_react\\projetos_react\\hooks\\src\\store\\index.js",[],"C:\\Users\\PinheirC\\hp_compliance\\projeto_react\\projetos_react\\hooks\\src\\store\\reducers\\index.js",[],"C:\\Users\\PinheirC\\hp_compliance\\projeto_react\\projetos_react\\hooks\\src\\store\\actions\\number.js",[],"C:\\Users\\PinheirC\\hp_compliance\\projeto_react\\projetos_react\\hooks\\src\\store\\actions\\index.js",[],"C:\\Users\\PinheirC\\hp_compliance\\projeto_react\\projetos_react\\hooks\\src\\store\\actions\\user.js",[],"C:\\Users\\PinheirC\\hp_compliance\\projeto_react\\projetos_react\\hooks\\src\\store\\reducers\\number.js",[],"C:\\Users\\PinheirC\\hp_compliance\\projeto_react